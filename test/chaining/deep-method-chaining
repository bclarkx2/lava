class C {
    var me = true;
    function get_val () {
        return me;
    }
}
class B {
    function make_c(){
        return new C();
    }
}
class A {
    var b = new B();
    function get_b(){
        return b;
    }
    static function main () {
        var a = new A();
        return 2;
    }
}
